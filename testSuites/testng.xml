<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite  name="Suite">

<listeners>
<listener class-name="rahulshettyacademy.TestComponents.Listeners"/>
</listeners>


  <test thread-count="5" name="Submit Order Test"><!-- Thread-count =5 means five browsers can at a time instances can open to execute your test cases -->
    <classes>
      <class name="rahulshettyacademy.tests.SubmitOrderTest"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Error Validations Test">
    <classes>
      <class name="rahulshettyacademy.tests.ErrorValidationsTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
